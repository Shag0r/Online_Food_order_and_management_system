@{
}
@if (TempData["Create"] != null)
{
    <div class=" text-end alert alert-dismissible alert-success w-25">
        <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
        <strong>@TempData["Create"]</strong>
    </div>

}

@if (TempData["Edit"] != null)
{
    <div class="alert alert-dismissible alert-warning w-25">
        <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
        <strong>@TempData["Edit"]</strong>
    </div>

}

@if (TempData["Delete"] != null)
{
    <div class=" alert alert-dismissible alert-danger w-25">
        <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
        <strong>@TempData["Delete"]</strong>
    </div>

}